{
    "version": 2,
    "builds": [
        {
            "src": "./index.ts",
            "use": "@vercel/node",
            "config": {
                "typescript": {
                    "tsconfigPath": "./tsconfig.json"
                }
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ]
}